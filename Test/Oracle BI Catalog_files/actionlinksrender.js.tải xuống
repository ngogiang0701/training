if(!saw.catalogaction.renderActionLinks){saw.catalogaction.renderActionLinks=function(a,b,j,e,g){if(!b.directLinkActions){b.directLinkActions=saw.catalogaction.getActionManager().getPrimaryActions(j);if(e){b.directLinkActions=b.directLinkActions.slice(0,e)}}var c=saw.createChildElement(a,"table");c.cellSpacing=0;c.cellPadding=0;var f=c.insertRow(0);for(var d=0;d<b.directLinkActions.length;d++){if(g&&obips.Array.contains(g,b.directLinkActions[d].id)){continue}saw.catalogaction.addActionLink(f,b,b.directLinkActions[d])}if(1){var h=j.caption;var k=saw.catalogaction.renderMoreLink(f.insertCell(f.cells.length),function(i){saw.catalogaction.showMoreActionsMenu(saw.getEvent(i),b,j,g);return false},saw.catalog.getLocalizedString("kmsgCatalogMoreActionsTip",h));obips.selenium.registerObject(b.itemInfo.path+"$"+saw.catalog.getLocalizedString("kmsgCatalogMoreActions"),k)}obips.selenium.registerObject(b.itemInfo.path+"$Container",a)};saw.catalogaction.renderActionLinksForNewCloudHome=function(a,b,j,e,g){b.directLinkActions={};var c=saw.createChildElement(a,"table");c.cellSpacing=0;c.cellPadding=0;var f=c.insertRow(0);for(var d=0;d<b.directLinkActions.length;d++){if(g&&obips.Array.contains(g,b.directLinkActions[d].id)){continue}saw.catalogaction.addActionLink(f,b,b.directLinkActions[d])}if(1){var h=j.caption;var k=saw.catalogaction.renderMoreLinkForNewCloudHome(f.insertCell(f.cells.length),function(i){saw.catalogaction.showMoreActionsMenu(saw.getEvent(i),b,j,g);return false},saw.catalog.getLocalizedString("kmsgCatalogMoreActionsTip",h));obips.selenium.registerObject(b.itemInfo.path+"$"+saw.catalog.getLocalizedString("kmsgCatalogMoreActions"),k)}obips.selenium.registerObject(b.itemInfo.path+"$Container",a)};saw.catalogaction.renderMoreLinkForNewCloudHome=function(d,a,e){var c=saw.createChildElement(d,"a","CatalogActionLink");c.href="javascript:void(null)";c.style.marginRight="20px";var b=saw.createChildElement(c,"img");b.border="0";b.src=obips.ResourceManager.getSingleton().getUR